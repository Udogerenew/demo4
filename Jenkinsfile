
stage('Build') {
    steps {
        echo 'Building the project...'
        bat 'echo Build step completed'
    }
}
stage('Test') {
    steps {
        echo 'Running tests...'
        bat 'echo Tests executed successfully'
    }
}
stage('Deploy') {
    steps {
        echo 'Deploying application...'
        bat 'echo Deployment completed'
    }
}
